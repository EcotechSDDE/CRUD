<h1>Editar Tempo</h1>
<form action="/tempos/<%= tempo.id %>?_method=PUT" method="POST">
    <div class="form-group">
        <label for="nome">Tempo</label>
        <input type="text" class="form-control" id="nome" name="nome" value="<%= tempo.nome %>" required>
    </div>
    <button type="submit" class="btn btn-warning">Salvar</button>
</form>
